# matrix of legal configurations
#
# --enable-loader --enable-elfsyms  bfd installed or pmbfd
#                                   support available
#
#   unset           unset           yes         --> enable_loader=yes; enable_elfsyms=no
#   unset           unset           no          --> enable_loader=no;  enable_elfsyms=yes
#   no              unset           yes         -->                    enable_elfsyms=yes
#   no              unset           no          -->       (*)          enable_elfsyms=yes
#   yes             unset           yes         -->                    enable_elfsyms=no;
#   yes             unset           no          --> ERROR(no BFD support available)
#   unset           no              yes         --> enable_loader=yes;
#   unset           no              no          --> enable_loader=no;
#   no              no              yes         --> OK
#   no              no              no          --> OK
#   yes             no              yes         --> OK
#   yes             no              no          --> ERROR(no BFD support available)
#   unset           yes             yes         --> enable_loader=no;
#   unset           yes             no          --> enable_loader=no;
#   no              yes             yes         --> OK
#   no              yes             no          --> OK
#   yes             yes             yes         --> ERROR(cannot enable both)
#   yes             yes             no          --> ERROR(cannot enable both)


{ $as_echo "$as_me:${as_lineno-$LINENO}: checking Checking consistency of configuration options --enable-loader and --enable-elfsyms" >&5
$as_echo_n "checking Checking consistency of configuration options --enable-loader and --enable-elfsyms... " >&6; }

if test "$enable_loader" = "yes" && test "$enable_elfsyms" = "yes"; then
	as_fn_error $? "Using both, --enable-loader and --enable-elfsyms, does not make sense (loader supersedes elfsyms)" "$LINENO" 5
fi

if test ! "$enable_loader" = "no" ; then
	if test "$have_bfd" = "yes" || test "$have_pmbfd" = "yes" ; then
		if test "$enable_elfsyms" = "yes" ; then
			enable_loader=no
		else
			enable_loader=yes
			enable_elfsyms=no
		fi
	else
		if test "$enable_loader" = "yes"; then
			as_fn_error $? "BFD sources not installed and pmbfd doesn't support your CPU; unable to enable the loader" "$LINENO" 5
		else
			enable_loader=no
			if test ! "$enable_elfsyms" = "no" ; then
				enable_elfsyms=yes
			fi
		fi
	fi
else
# enable_loader = no
	if test ! "${enable_elfsyms}" = "no"; then
		enable_elfsyms=yes;
	fi
fi
{ $as_echo "$as_me:${as_lineno-$LINENO}: result: passed" >&5
$as_echo "passed" >&6; }

{ $as_echo "$as_me:${as_lineno-$LINENO}: enable_loader: $enable_loader, enable_elfsyms: $enable_elfsyms" >&5
